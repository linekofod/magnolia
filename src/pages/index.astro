<style>
  .bg_image {
    position: relative;
    width: 100%;
    height: fit-content;
    display: flex;
    justify-content: center;
  }
  .bg_image:before {
    content: " ";
    display: inline-block;
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-image: url("/public/imgs/index/index1.webp");
    background-repeat: no-repeat;
    background-size: cover;
    opacity: 0.2;
  }
</style>

<script>
  
  // CAROUSEL - VORES BUKETTER
 

  // DYNAMISK TEKSTSKIFT
  document.addEventListener("DOMContentLoaded", () => {
    const dynamicText = document.querySelector(".text-dynamic");
    const phrases = [
      "DEN NYBAGTE MOR",
      "FØDSELAREN",
      "TEAMET",
      "DIN KÆRE",
      "DIN MOR",
      "JUBILAREN",
      "ARBEJDSPLADSEN",
      "DIN MORMOR",
      "DIN SØSTER",
      "KONTORET",
      "STUDENTEN",
      "DINE KOLLEGAER",
    ];

    let index = 0;

    function updateText() {
      dynamicText.textContent = phrases[index];
      index = (index + 1) % phrases.length; // Loop tilbage til start
    }

    setInterval(updateText, 1500); // Skift tekst hver 3. sekund
  });

  // INSTAGRAM CAROUSEL

  const carousel = document.getElementById('carousel');
  const scrollByAmount = carousel.clientWidth;
  let autoScrollInterval;

  // Funktion til automatisk rulning
  const autoScroll = () => {
    carousel.scrollBy({ left: scrollByAmount, behavior: 'smooth' });

  // Tjek om vi er ved slutningen og reset til start
  if (carousel.scrollLeft + carousel.clientWidth >= carousel.scrollWidth) {
    setTimeout(() => {
      carousel.scrollTo({ left: 0, behavior: 'smooth' });
    }, 600); // Pause før reset
  }
};

  // Start automatisk rulning
  autoScrollInterval = setInterval(autoScroll, 6000); // Skifter hvert 6. sekund

  // Stop rulning ved hover
  carousel.addEventListener('mouseenter', () => clearInterval(autoScrollInterval));

  // Genoptag rulning når musen forlader karrusellen
  carousel.addEventListener('mouseleave', () => {
    autoScrollInterval = setInterval(autoScroll, 3000);
  });

  // ANIMATIONER
  import { inView, animate } from "motion";

  /* inView & return på grønnere valg */
  inView(".inviewfadein1", ({ target }) => {
    animate(target, { opacity: [0, 1] }, { duration: 1 });
    return () => {
      animate(target, { opacity: 1 });
    };
  });

  inView(".inviewfadein2", ({ target }) => {
    animate(target, { opacity: [0, 1] }, { delay: 0.5, duration: 1 });
    return () => {
      animate(target, { opacity: 1 });
    };
  });

